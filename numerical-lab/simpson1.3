import math

def f(x):
    return math.sin(x)

def simpsons_13_rule(a, b, n):
    if n % 2 == 0:
        h = (b - a) / n
        sum_result = f(a) + f(b)

        for i in range(1, n, 2):
            sum_result += 4 * f(a + i * h)

        for i in range(2, n - 1, 2):
            sum_result += 2 * f(a + i * h)

        result = (h / 3) * sum_result
        return result
    else:
        return "n must be even!"

a = float(input("Enter the lower limit of integration (a): "))
b = float(input("Enter the upper limit of integration (b): "))
n = int(input("Enter the number of subintervals (must be even): "))

result = simpsons_13_rule(a, b, n)

if isinstance(result, str):
    print(result)
else:
    print(f"The approximate integral of the function is: {result}")
